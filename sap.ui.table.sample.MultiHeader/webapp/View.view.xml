<mvc:View xmlns:core="sap.ui.core"
	controllerName="sap.ui.table.sample.MultiHeader.Controller"
	xmlns="sap.ui.table"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:m="sap.m"
	height="100%">
	<m:Page
		showHeader="false"
		enableScrolling="false"
		class="sapUiContentPadding">
		<m:content>
			<Table
				id="table1"
				selectionMode="None"
				columnHeaderVisible="true"
				columnHeaderHeight="30%"
				
				rows="{/modelData}"
				enableColumnReordering="false"
				
				>
				<extension>
					<m:OverflowToolbar style="Clear">
						<m:Title id="title" text="Contacts"/>
					</m:OverflowToolbar>
				</extension>
				<columns>
					<Column	width="5rem" id="supplierColumn" resizable="false">
						<m:Text text="No" textAlign="Center" width="100%"/>
							<template>
								<m:Text text="1" textAlign="Center" width="100%"/>
							</template>
					</Column>
				<Column
					width="5rem" id="supplierColumn2" resizable="false">
					<m:Text text="구분" textAlign="Center" width="100%"/>
					<template>
						<m:Text text="True/False"/>
					</template>
				</Column>
					<Column
						width="10rem"
						headerSpan="11"
						resizable="false">
						<multiLabels>
							<m:Label class="contractDetail" text="계약사항" textAlign="Center" width="100%"/>
							<m:Label text="견적번호" textAlign="Center" width="100%"/>
						</multiLabels>
						<template>
							<m:Text text="{street}" wrapping="false"/>
						</template>
					</Column>
					<Column
						width="5rem"
						resizable="false">
						<multiLabels>
							<m:Label visible="false"/>
							<m:Label text="상품코드" textAlign="Center" width="100%"/>
						</multiLabels>
						<template>
							<m:Input value="{phone}"/>
						</template>
					</Column>
					<Column
						width="5rem"
						resizable="false">
						<multiLabels>
							<m:Label visible="false"/>
							<m:Label text="계약일자" textAlign="Center" width="100%"/>
						</multiLabels>
						<template>
							<m:Input value="{phone}"/>
						</template>
					</Column>
					<Column
						width="5rem"
						resizable="false">
						<multiLabels>
							<m:Label visible="false"/>
							<m:Label text="계정수" textAlign="Center" width="100%"/>
						</multiLabels>
						<template>
							<m:Input value="{phone}"/>
						</template>
					</Column>
					<Column
						width="5rem"
						resizable="false">
						<multiLabels>
							<m:Label visible="false"/>
							<m:Label text="고객명" textAlign="Center" width="100%"/>
						</multiLabels>
						<template>
							<m:Input value="{phone}"/>
						</template>
					</Column>
					<Column
						width="5rem"
						resizable="false">
						<multiLabels>
							<m:Label visible="false"/>
							<m:Label text="고객번호" textAlign="Center" width="100%"/>
						</multiLabels>
						<template>
							<m:Input value="{phone}"/>
						</template>
					</Column>
					<Column
						width="5rem"
						resizable="false">
						<multiLabels>
							<m:Label visible="false"/>
							<m:Label text="총횟수" textAlign="Center" width="100%"/>
						</multiLabels>
						<template>
							<m:Input value="{phone}"/>
						</template>
					</Column>
					<Column
						width="5rem"
						resizable="false">
						<multiLabels>
							<m:Label visible="false"/>
							<m:Label text="고객요청일자" textAlign="Center" width="100%"/>
						</multiLabels>
						<template>
							<m:Input value="{phone}"/>
						</template>
					</Column>
					<Column
						width="5rem"
						resizable="false">
						<multiLabels>
							<m:Label visible="false"/>
							<m:Label text="고객요청시간" textAlign="Center" width="100%"/>
						</multiLabels>
						<template>
							<m:Input value="{phone}"/>
						</template>
					</Column>
					<Column
						width="3rem"
						resizable="false">
						<multiLabels>
							<m:Label visible="false"/>
							<m:Label text="지역" textAlign="Center" width="100%"/>
						</multiLabels>
						<template>
							<m:Input value="{phone}"/>
						</template>
					</Column>
					<!-- TEST부 -->
					<Column
						width="3rem"
						resizable="false">
						<multiLabels>
							<m:Label visible="false"/>
							<m:Label text="지역" textAlign="Center" width="100%"/>
						</multiLabels>
						<template>
							<m:Input value="{phone}"/>
						</template>
					</Column>
					<Column
						width="10rem"
						headerSpan="9"
						resizable="false">
						<multiLabels> 
							<m:Label class="assginedDetail" text="배정사항" textAlign="Center" width="100%"/>
							<m:Label text="담당자" textAlign="Center" width="100%"/>
						</multiLabels>
						<template>
							<m:Input value="{phone}"/>
						</template>
					</Column>
					<Column
						width="auto"
						resizable="false">
						<multiLabels>
							<m:Label text="배정사항"/>
							<m:Label text="주차" textAlign="Center" width="100%"/>
						</multiLabels>
						<template>
							<m:Input value="{phone}"/>
						</template>
					</Column>
					<Column
						width="auto"
						resizable="false">
						<multiLabels>
							<m:Label text="배정사항"/>
							<m:Label text="요일" textAlign="Center" width="100%"/>
						</multiLabels>
						<template>
							<m:Input value="{phone}"/>
						</template>
					</Column>
					<Column
						width="auto"
						resizable="false">
						<multiLabels>
							<m:Label text="배정사항"/>
							<m:Label text="총수행" textAlign="Center" width="100%"/>
						</multiLabels>
						<template>
							<m:Input value="{phone}"/>
						</template>
					</Column>
					<Column
						width="auto"
						resizable="false">
						<multiLabels>
							<m:Label text="배정사항"/>
							<m:Label text="해충이슈" textAlign="Center" width="100%"/>
						</multiLabels>
						<template>
							<m:Input value="{phone}"/>
						</template>
					</Column>
					<Column
						width="auto"
						resizable="false">
						<multiLabels>
							<m:Label text="배정사항"/>
							<m:Label text="최종방문일자" textAlign="Center" width="auto"/>
						</multiLabels>
						<template>
							<m:Input value="{phone}"/>
						</template>
					</Column>
					<Column
						width="auto"
						resizable="false">
						<multiLabels>
							<m:Label text="배정사항"/>
							<m:Label text="최종방문시간" textAlign="Center" width="100%"/>
						</multiLabels>
						<template>
							<m:Input value="{phone}"/>
						</template>
					</Column>
					<Column
						width="auto"
						resizable="false">
						<multiLabels>
							<m:Label text="배정사항"/>
							<m:Label text="수정일자" textAlign="Center" width="100%"/>
						</multiLabels>
						<template>
							<m:Text text="test"/>
						</template>
					</Column>
					<Column
						width="auto"
						resizable="false">
						<multiLabels>
							<m:Label text="배정사항"/>
							<m:Label text="한성" textAlign="Center" width="100%"/>
						</multiLabels>
						<template>
							<m:Text text="test"/>
						</template>
					</Column>
				</columns>
			</Table>
		</m:content>
	</m:Page>
</mvc:View>